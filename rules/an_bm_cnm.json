{
    "machine": "PDA",
    "description": "PDA recognizable grammar `a^nb^mc^(n+m)`",
    "symbols": ["^", "a", "b", "c", "x", "y", "$"],
    "rules": [
        [["^", "INITIAL", null], ["Q1", ["push", "$"]]],
        [["$", "Q3", "$"], ["ACCEPT", "pop"]],
        [["a", "Q1", "$"], ["Q1", ["push", "A"]]],
        [["a", "Q1", "A"], ["Q1", ["push", "A"]]],
        [["b", "Q1", "A"], ["Q2", ["push", "B"]]],
        [["b", "Q2", "A"], ["Q2", ["push", "B"]]],
        [["b", "Q2", "B"], ["Q2", ["push", "B"]]],
        [["c", "Q2", "B"], ["Q3", "pop"]],
        [["c", "Q3", "A"], ["Q3", "pop"]],
        [["c", "Q3", "B"], ["Q3", "pop"]]
    ]
}
