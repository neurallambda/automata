{
    "machine": "PDA",
    "description": "PDA recognizable grammar `a^nb^n or a^n|b^n`",
    "symbols": ["^", "a", "b", "x", "y", "|", "$"],
    "rules": [
        [["^", "INITIAL", null], ["Q1", ["push", "$"]]],
        [["$", "Q1", "$"], ["ACCEPT", "pop"]],
        [["a", "Q1", "$"], ["Q1", ["push", "A"]]],
        [["a", "Q1", "A"], ["Q1", ["push", "A"]]],
        [["|", "Q1", "A"], ["Q2", "nullop"]],
        [["b", "Q1", "A"], ["Q2", "pop"]],
        [["b", "Q2", "A"], ["Q2", "pop"]],
        [["$", "Q2", "$"], ["ACCEPT", "pop"]]
    ]
}
